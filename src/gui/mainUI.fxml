<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainUI">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="400.0" spacing="10.0">
         <Label text="Trash Games">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <TabPane prefHeight="340.0" prefWidth="400.0" tabClosingPolicy="UNAVAILABLE">
           <tabs>
             <Tab text="%chn_chess">
               <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="320.0" prefWidth="200.0">
                        <children>
                           <VBox alignment="CENTER" prefHeight="328.0" prefWidth="400.0" spacing="20.0">
                              <children>
                                 <Label text="%chn_chess">
                                    <font>
                                       <Font size="18.0" />
                                    </font></Label>
                                    <Button mnemonicParsing="false" onAction="#doubleGame" text="%double_game" />
                                    <Button mnemonicParsing="false" onAction="#lanGame" text="%lan_game" />
                              </children></VBox>
                        </children></AnchorPane>
               </content>
             </Tab>
             <Tab text="Untitled Tab 2">
               <content>
                 <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
             </Tab>
           </tabs>
         </TabPane>
        </VBox>
    </children>
</AnchorPane>
